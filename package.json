{
  "name": "react-tutorial",
  "version": "0.0.1",
  "description": "A repository covering React, Flux (with Flummox), Immutable JS, Isomorphism and Node JS ",
  "main": "index.js",
  "scripts": {
    "start": "webpack -w & npm run serve",
    "serve": "cd dist && python -m SimpleHTTPServer",
    "test": "node_modules/.bin/mocha --compilers js:babel/register tests",
    "lint": "npm run lint-src && npm run lint-tests",
    "lint-src": "node_modules/.bin/eslint --ext .js --ext .jsx src",
    "lint-tests": "node_modules/.bin/eslint --ext .js --ext .jsx tests"
  },
  "author": "Jack Callister",
  "license": "ISC",
  "dependencies": {
    "worsley": "0.0.1-alpha.2",
    "babel": "^5.2.16",
    "ejs": "^2.3.1",
    "express": "^4.12.3",
    "react": "^0.13.2",
    "react-router": "^0.13.3"
  },
  "devDependencies": {
    "babel-core": "^5.2.16",
    "babel-eslint": "^3.1.1",
    "babel-loader": "^5.0.0",
    "babel-runtime": "^5.2.17",
    "eslint": "^0.20.0",
    "eslint-plugin-react": "^2.2.0",
    "jsdom": "^5.3.0",
    "mocha": "^2.2.4",
    "webpack": "^1.8.11"
  }
}
